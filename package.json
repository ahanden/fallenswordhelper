{
  "name": "fallenswordhelper",
  "version": "15.20.2",
  "description": "Fallen Sword Helper is a Greasemonkey script meant to enhance the experience of playing Fallen Sword, by removing repetitive tasks, presenting more information at a glance, enhancing the social aspect of the game and creating shortcuts to common tasks.",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run test || exit 0",
    "build": "npm run clean && npm run rollup && npm run uglify",
    "build:beta": "npm run prebuild && npm run rollup:beta && npm run uglify:beta",
    "build:dev": "npm run prebuild && npm run rollup:dev && npm run uglify:dev",
    "build:prod": "npm run prebuild && npm run rollup:prod && npm run uglify:prod",
    "clean": "npm run clean:dev && npm run clean:beta && npm run clean:prod",
    "clean:beta": "rimraf dist/beta/*",
    "clean:dev": "rimraf dist/dev/*",
    "clean:prod": "rimraf dist/prod/*",
    "eslint": "eslint .",
    "jshint": "jshint . --verbose",
    "jsinspect": "jsinspect ./src",
    "minor": "npm --no-git-tag-version version minor",
    "patch": "npm --no-git-tag-version version patch",
    "rollup": "npm run rollup:dev && npm run rollup:beta && npm run rollup:prod",
    "rollup:beta": "npm run rollup:betacalf && npm run rollup:betafsh",
    "rollup:dev": "npm run rollup:devcalf && npm run rollup:devfsh",
    "rollup:prod": "npm run rollup:prodcalf && npm run rollup:prodfsh",
    "rollup:betacalf": "rollup -c --environment FOLDER:beta,SOURCE:calf",
    "rollup:devcalf": "rollup -c --environment FOLDER:dev,SOURCE:calf",
    "rollup:prodcalf": "rollup -c --environment FOLDER:prod,SOURCE:calf",
    "rollup:betafsh": "rollup -c --environment FOLDER:beta,SOURCE:fsh",
    "rollup:devfsh": "rollup -c --environment FOLDER:dev,SOURCE:fsh",
    "rollup:prodfsh": "rollup -c --environment FOLDER:prod,SOURCE:fsh",
    "serve": "run-p watch ws",
    "silence": "npm config set loglevel silent",
    "test": "npm run jshint && npm run eslint",
    "uglify": "npm run uglify:dev && npm run uglify:beta && npm run uglify:prod",
    "uglify:beta": "uglifyjs dist/beta/calfSystem.js --stats -m -c unsafe,unsafe_comps,unsafe_math,unsafe_proto -o dist/beta/calfSystem.min.js",
    "postuglify:beta": "cost :max,unzip:dist/beta/calfSystem.min.js && cost :max:dist/beta/calfSystem.min.js",
    "uglify:dev": "uglifyjs dist/dev/calfSystem.js -c unsafe,unsafe_comps,unsafe_math,unsafe_proto -m --stats -o dist/dev/calfSystem.min.js",
    "postuglify:dev": "cost :max,unzip:dist/dev/calfSystem.min.js && cost :max:dist/dev/calfSystem.min.js",
    "uglify:prod": "uglifyjs dist/prod/calfSystem.js --stats -m -c unsafe,unsafe_comps,unsafe_math,unsafe_proto -o dist/prod/calfSystem.min.js",
    "postuglify:prod": "cost :max,unzip:dist/prod/calfSystem.min.js && cost :max:dist/prod/calfSystem.min.js",
    "watch": "chokidar-cmd -c \"npm run rollup:devcalf\" -t src --initial",
    "ws": "http-server -p 9966 -c-1 -S -C cert.crt -K key.pem"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fallenswordhelper/fallenswordhelper.git"
  },
  "keywords": [
    "fallensword",
    "fallenswordhelper"
  ],
  "author": "fallenswordhelper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fallenswordhelper/fallenswordhelper/issues"
  },
  "homepage": "https://fallenswordhelper.github.io/fallenswordhelper",
  "devDependencies": {
    "chokidar-cmd": "^1.2.1",
    "cost": "^1.2.3",
    "eslint": "^4.4.1",
    "http-server": "^0.10.0",
    "ip": "^1.1.5",
    "jshint": "^2.9.5",
    "jsinspect": "^0.12.6",
    "jsonlint": "^1.6.2",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.47.4",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-jscc": "^0.3.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify-js": "^3.0.27"
  },
  "dependencies": {}
}
