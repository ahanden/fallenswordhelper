{
  "name": "fallenswordhelper",
  "version": "15.23.106",
  "description": "Fallen Sword Helper is a Greasemonkey script meant to enhance the experience of playing Fallen Sword, by removing repetitive tasks, presenting more information at a glance, enhancing the social aspect of the game and creating shortcuts to common tasks.",
  "scripts": {
    "addsourceroot": "node scripts/addSourceRoot.js",
    "prebuild": "npm run test",
    "build": "npm run rollup",
    "postbuild": "npm run addsourceroot && npm run getsize",
    "precommit": "npm run test",
    "predevbuild": "npm outdated",
    "devbuild": "npm run build",
    "preeslint": "echo Starting eslint",
    "eslint": "eslint . --cache",
    "posteslint": "echo Finished eslint",
    "getsize": "npm run getsizedev && npm run getsizebeta && npm run getsizeprod",
    "getsizedev": "node scripts/getCalfSize.js dev",
    "getsizebeta": "node scripts/getCalfSize.js beta",
    "getsizeprod": "node scripts/getCalfSize.js prod",
    "minor": "npm --no-git-tag-version version minor",
    "patch": "npm --no-git-tag-version version patch",
    "prepush": "npm run test",
    "prerollup": "echo Starting rollup",
    "rollup": "npm run rollupfsh && npm run rollupcalf",
    "postrollup": "echo Finished rollup",
    "rollupcalf": "npm run rollupcalfdev && npm run rollupcalfbeta && npm run rollupcalfprod",
    "rollupcalfdev": "rollup -c scripts/calfdev.js",
    "rollupcalfbeta": "rollup -c scripts/calfbeta.js",
    "rollupcalfprod": "rollup -c scripts/calfprod.js",
    "rollupfsh": "npm run rollupfshdev && npm run rollupfshbeta && npm run rollupfshprod",
    "rollupfshdev": "rollup -c scripts/fshdev.js",
    "rollupfshbeta": "rollup -c scripts/fshbeta.js",
    "rollupfshprod": "rollup -c scripts/fshprod.js",
    "rollupfshwatch": "rollup -c scripts/fshwatch.js",
    "silence": "npm config set loglevel silent",
    "test": "npm run eslint",
    "watch": "npm run rollupfshwatch && rollup -c scripts/calfwatch.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fallenswordhelper/fallenswordhelper.git"
  },
  "keywords": [
    "fallensword",
    "fallenswordhelper"
  ],
  "author": "fallenswordhelper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fallenswordhelper/fallenswordhelper/issues"
  },
  "homepage": "https://fallenswordhelper.github.io/fallenswordhelper",
  "devDependencies": {
    "eslint": "^5.14.1",
    "gzip-size": "^5.0.0",
    "husky": "^1.3.1",
    "rollup": "^1.3.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-jscc": "^1.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "trucker": "^0.8.3"
  }
}
