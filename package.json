{
  "name": "fallenswordhelper",
  "version": "15.23.70",
  "description": "Fallen Sword Helper is a Greasemonkey script meant to enhance the experience of playing Fallen Sword, by removing repetitive tasks, presenting more information at a glance, enhancing the social aspect of the game and creating shortcuts to common tasks.",
  "scripts": {
    "build": "npm run test && npm run rollup && npm run uglify",
    "precommit": "npm run test",
    "devbuild": "npm outdated && npm run build",
    "eslint": "echo Starting eslint && eslint . --cache && echo Finished eslint",
    "minor": "npm --no-git-tag-version version minor",
    "patch": "npm --no-git-tag-version version patch",
    "prepush": "npm run test",
    "prerollup": "echo Starting rollup",
    "rollup": "npm run rollup:dev && npm run rollup:beta && npm run rollup:prod",
    "postrollup": "echo Finished rollup",
    "rollup:beta": "rollup -c scripts/betafsh.js && rollup -c scripts/betacalf.js",
    "rollup:dev": "rollup -c scripts/devfsh.js && rollup -c scripts/devcalf.js",
    "rollup:prod": "rollup -c scripts/prodfsh.js && rollup -c scripts/prodcalf.js",
    "silence": "npm config set loglevel silent",
    "test": "npm run eslint",
    "preuglify": "echo Starting uglify",
    "uglify": "npm run uglify:dev && npm run uglify:beta && npm run uglify:prod",
    "postuglify": "echo Finished uglify",
    "uglify:beta": "node scripts/uglifyBeta.js",
    "postuglify:beta": "node scripts/getCalfSize.js beta",
    "uglify:dev": "node scripts/uglifyDev.js",
    "postuglify:dev": "node scripts/getCalfSize.js dev",
    "uglify:prod": "node scripts/uglifyProd.js",
    "postuglify:prod": "node scripts/getCalfSize.js prod",
    "watch": "rollup -c scripts/devcalfwatch.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fallenswordhelper/fallenswordhelper.git"
  },
  "keywords": [
    "fallensword",
    "fallenswordhelper"
  ],
  "author": "fallenswordhelper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fallenswordhelper/fallenswordhelper/issues"
  },
  "homepage": "https://fallenswordhelper.github.io/fallenswordhelper",
  "devDependencies": {
    "eslint": "^5.11.1",
    "gzip-size": "^5.0.0",
    "husky": "^1.3.1",
    "rollup": "^1.0.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-jscc": "^1.0.0",
    "rollup-plugin-serve": "^0.6.1",
    "trucker": "^0.8.3",
    "uglify-js": "^3.4.9"
  }
}
