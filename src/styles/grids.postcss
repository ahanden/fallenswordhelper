/* Mailbox & Crafting & Forging */
.fshTakeGrid,
.fshItemGrid {
  display: grid;
  grid-gap: 2px;
  grid-template-columns: repeat(9, 60px);
  justify-content: center;
}

.fshItemGrid div,
.fshTakeGrid div div:nth-child(1) {
  align-items: center;
  background: url('https://cdn.fallensword.com/inventory/2x3.gif');
  display: flex;
  height: 90px;
  justify-content: center;
  width: 60px;
}
